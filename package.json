{
  "name": "swapspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:backend": "npm run dev --workspace=packages/backend",
    "dev:app": "npm run dev --workspace=packages/app",
    "build:backend": "npm run build --workspace=packages/backend",
    "build:app": "npm run build --workspace=packages/app",
    "build:cap": "npm run build:cap --workspace=packages/app",
    "cap:sync": "npm run cap:sync --workspace=packages/app",
    "cap:ios": "npm run cap:ios --workspace=packages/app",
    "cap:android": "npm run cap:android --workspace=packages/app"
  }
}
