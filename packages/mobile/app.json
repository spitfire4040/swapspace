{
  "expo": {
    "name": "SwapSpace",
    "slug": "swapspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swapspace.app"
    },
    "android": {
      "package": "com.swapspace.app"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow SwapSpace to access your photos."
        }
      ]
    ]
  }
}
